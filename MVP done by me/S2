import pandas as pd
import re
from random import choice, randint
from concurrent.futures import ThreadPoolExecutor
from selenium import webdriver
def check_email_phone_on_link(email, phone, source_link, headers):
# create a webdriver instance and open the source link
driver = webdriver.Chrome()
driver.get(source_link)
# get the page source
page_source = driver.page_source
# check if the email and phone are found on the page using regular expressions
if not re.search(email, page_source) or not re.search(phone, page_source):
return 'no'
else:
return 'yes'

